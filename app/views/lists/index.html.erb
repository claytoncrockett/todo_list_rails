<div class="center">
<h1> Items in List </h1>
<p><%= link_to 'New Item', new_list_path,{class: "btn pulse"} %></p>
<% @lists.each do |list| %>
<fieldset>
  <h2><%= list.title %></h2>
  <h4>Category: <%= list.category %></h4>
  <%= link_to 'Show Item', list_path(list), {class: "btn teal"} %>
  <%= link_to 'Edit Item', edit_list_path(list), {class: "btn purple"} %>
  <%= link_to 'Delete Item', list, {method: :delete, class: "btn blue"} %>
  </fieldset>
<% end %>
</div>